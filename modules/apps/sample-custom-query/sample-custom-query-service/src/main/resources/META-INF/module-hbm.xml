	<class name="com.liferay.sample.custom.query.model.impl.CustomImpl" table="SAMPLE_Custom">
		<id name="customId" type="long">
			<generator class="assigned" />
		</id>
		<property name="customName" type="com.liferay.portal.dao.orm.hibernate.StringType" />
	</class>
</hibernate-mapping>